
    // Import the data
electrondensity = Import("watermolecule");
    // Partition the data
electrondensity = Partition(electrondensity);
    // Create an isosurface at a value of 0.3
isosurface = Isosurface(electrondensity,0.3);
    // Create a camera
camera = AutoCamera(isosurface);
    // Display
Display(isosurface,camera);
    // Wait for keyin before continuing
KeyIn("press enter when you are ready to continue");


    // continue
isosurface = Isosurface(electrondensity,0.5);
camera = AutoCamera(isosurface);
Display(isosurface,camera);




